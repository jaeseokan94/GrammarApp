<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/bkg"
    tools:context="com.example.spanishgrammarapp.PracticeActivity">

    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton"
        android:background="@drawable/greetings"
        android:layout_marginTop="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton2"
        android:layout_marginTop="40dp"
        android:background="@drawable/checkin"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton3"
        android:layout_marginTop="40dp"
        android:background="@drawable/sight"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true" />



    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton4"
        android:background="@drawable/directions"
        android:layout_below="@+id/imageButton"
        android:layout_marginTop="80dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />

    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton5"
        android:background="@drawable/eating"
        android:layout_alignBottom="@+id/imageButton4"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton6"
        android:background="@drawable/likes"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3" />

    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton7"
        android:background="@drawable/planning"
        android:layout_below="@+id/imageButton4"
        android:layout_marginTop="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />

    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton8"
        android:background="@drawable/shopping"
        android:layout_alignBottom="@+id/imageButton7"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="100"
        android:layout_height="100"
        android:id="@+id/imageButton9"
        android:background="@drawable/dating"
        android:layout_alignTop="@+id/imageButton8"
        android:layout_alignLeft="@+id/imageButton6"
        android:layout_alignStart="@+id/imageButton6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton10"
        android:background="@drawable/greetings2"
        android:layout_below="@+id/imageButton"
        android:layout_alignRight="@+id/imageButton2"
        android:layout_alignEnd="@+id/imageButton2" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton11"
        android:background="@drawable/checkin2"
        android:layout_below="@+id/imageButton2"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton12"
        android:background="@drawable/sightseeing2"
        android:layout_below="@+id/imageButton3"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton13"
        android:background="@drawable/directions2"
        android:layout_below="@+id/imageButton4"
        android:layout_alignRight="@+id/imageButton5"
        android:layout_alignEnd="@+id/imageButton5" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton14"
        android:background="@drawable/eating2"
        android:layout_below="@+id/imageButton5"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton15"
        android:background="@drawable/likes2"
        android:layout_below="@+id/imageButton6"
        android:layout_alignLeft="@+id/imageButton6"
        android:layout_alignStart="@+id/imageButton6" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton16"
        android:background="@drawable/planning2"
        android:layout_below="@+id/imageButton7"
        android:layout_alignRight="@+id/imageButton8"
        android:layout_alignEnd="@+id/imageButton8" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton17"
        android:background="@drawable/shopping2"
        android:layout_alignBottom="@+id/imageButton16"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton18"
        android:background="@drawable/dating2"
        android:layout_below="@+id/imageButton9"
        android:layout_alignLeft="@+id/imageButton9"
        android:layout_alignStart="@+id/imageButton9"/>
</RelativeLayout>
